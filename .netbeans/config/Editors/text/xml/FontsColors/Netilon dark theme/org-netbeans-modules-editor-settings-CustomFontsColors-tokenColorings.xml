<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor default="whitespace" name="xml-ws"/>
    <fontcolor default="operator" foreColor="ff6a8759" name="xml-operator"/>
    <fontcolor default="entity-reference" name="xml-ref"/>
    <fontcolor default="default" foreColor="ff86c365" name="xml-pi-start"/>
    <fontcolor default="default" foreColor="ff00cc99" name="xml-value"/>
    <fontcolor default="markup-element" foreColor="ff90c175" name="xml-tag"/>
    <fontcolor default="default" foreColor="ff5c8d41" name="xml-doctype"/>
    <fontcolor default="default" name="xml-text"/>
    <fontcolor default="default" name="xml-cdata-section"/>
    <fontcolor default="error" name="xml-error"/>
    <fontcolor default="default" name="xml-pi-end">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor default="default" foreColor="lightGray" name="xml-pi-target">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="comment" name="xml-comment"/>
    <fontcolor default="markup-attribute" name="xml-attribute"/>
    <fontcolor default="whitespace" name="xml-EOL"/>
    <fontcolor default="default" name="xml-pi-content"/>
</fontscolors>
